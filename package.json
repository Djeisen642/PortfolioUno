    "uikit": "^3.0.0-beta.16",
    "url-loader": "^0.5.7",
    "vue": "^2.2.1",
    "vue-html-loader": "^1.2.4",
    "vue-loader": "^11.3.4",
    "vue-router": "^2.4.0",
    "vue-template-compiler": "^2.2.6",
